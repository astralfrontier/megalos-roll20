each val in [1, 2, 3, 4, 5]
  .cell
    +box_default(`Aether Current ${val}`, "")
      select(name=`attr_aether_current_${val}`)
        option Available
        option Used
        option Throne Damage
        option Astral Seal
        option Umbral Seal
        option Weak Charge
        option Surging Charge
      button.sheet-blank-button.noctis-button-red(type="action" name="act_aethercurrent" data-aethercurrent-number=val)